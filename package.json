{
  "name": "pdf-invoices-edit-woo-address",
  "version": "1.0.0",
  "description": "Enhance your WooCommerce experience with our plugin that seamlessly integrates with PDF Invoices. Now, easily modify the billing address of your orders before generating PDF Invoices. Our user-friendly solution conveniently places an edit button within the billing section of each order in the my-account area for quick and efficient updates. Elevate your order management process with this essential feature.",
  "main": "index.js",
  "scripts": {
    "i18n": "grunt textdomain && wp i18n make-pot . --exclude=\".github,scripts,config,node_modules,.vscode,.eslintrc.js,.prettierrc.js,package.json,package-lock.json,tests\" languages/pdf-invoices-edit-woo-address.pot --domain=pdf-invoices-edit-woo-address --debug",
    "pretty": "prettier --check .",
    "pretty:fix": "prettier --write .",
    "lint-js": "wp-scripts lint-js",
    "lint-js:fix": "npm run pretty:fix && npm run lint-js -- --fix",
    "lint-css": "wp-scripts lint-style --allow-empty-input",
    "lint-css:fix": "npm run lint-css -- --fix",
    "packages-update": "wp-scripts packages-update"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sarangshahane/pdf-invoices-edit-woo-address.git"
  },
  "bugs": {
    "url": "https://github.com/sarangshahane/pdf-invoices-edit-woo-address/issues"
  },
  "homepage": "https://github.com/sarangshahane/pdf-invoices-edit-woo-address#readme",
  "devDependencies": {
    "@wordpress/eslint-plugin": "^9.3.0",
    "@wordpress/scripts": "^21.0.1",
    "grunt": "^1.4.1",
    "grunt-bumpup": "^0.6.3",
    "grunt-cli": "^1.4.3",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-compress": "^2.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-text-replace": "^0.4.0",
    "grunt-wp-i18n": "^1.0.3",
    "prettier": "npm:wp-prettier@2.2.1-beta-1",
    "wp-prettier": "^2.6.2"
  },
  "author": "Sarang A. Shahane",
  "license": "GPL-3.0-or-later",
  "dependencies": {}
}
